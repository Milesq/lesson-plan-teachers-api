{
    "env": {
        "node": true,
        "jasmine": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "warn",
            "windows"
        ],
        "indent": [
            "error",
            4,
            {
                "outerIIFEBody": 0
            }
        ],
        "no-unused-vars": "warn",
        "no-console": "off"
    }
}